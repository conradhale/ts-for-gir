{
	"name": "@ts-for-gir-example/gtk-4-application-example",
	"version": "4.0.0-beta.26",
	"description": "Simple GJS Gtk 4 example app to demonstrate how you can use and extend Gtk.Application",
	"type": "module",
	"private": true,
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "node --experimental-specifier-resolution=node --experimental-strip-types --experimental-transform-types --no-warnings esbuild.ts",
