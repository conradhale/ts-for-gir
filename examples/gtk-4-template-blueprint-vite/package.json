	"license": "MIT",
	"devDependencies": {
		"@gjsify/vite-plugin-blueprint": "^0.2.2",
		"typescript": "^5.9.2",
		"vite": "^7.1.4"
	},
	"dependencies": {
		"@girs/gio-2.0": "workspace:^",
		"@girs/gjs": "workspace:^",
		"@girs/glib-2.0": "workspace:^",
		"@girs/gtk-4.0": "workspace:^"
	}
}
